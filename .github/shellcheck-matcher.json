{
  "problemMatcher": [
    {
      "owner": "shellcheck",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\s(note|warning|error):\\s(.*)\\s\\[(SC\\d+)\\]$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 5,
          "code": 6
        }
      ]
    }
  ]
}
